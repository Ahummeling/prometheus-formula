#########################################################
# File managed by Salt. Changes risk being overwritten.
#########################################################
[Unit]
Description={{ desc }}
Wants={{ wants }}
After={{ after }}
Documentation=https://github.com/saltstack-formulas/prometheus-formula

[Service]
User={{ user }}
Group={{ group }}
ExecStart={{ start }}
ExecStop={{ stop }}
PIDFile=/var/run/{{ name }}.pid

[Install]
WantedBy=multi-user.target
